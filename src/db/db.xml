<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd"
    objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="seats" author="sg">
        <createTable tableName="seats">
            <column name="id" type="smallint"><constraints primaryKey="true"/></column>
            <column name="owner" type="varchar(255)"><constraints nullable="true"/></column>
        </createTable>

        <createIndex tableName="seats" indexName="seats_id">
            <column name="id"/>
        </createIndex>
    </changeSet>

    <changeSet id="test data" author="sg" context="test-data">
        <sql>insert into seats (id) values (1), (2), (3), (4), (5), (6), (7), (8), (9) </sql>
    </changeSet>

</databaseChangeLog>
